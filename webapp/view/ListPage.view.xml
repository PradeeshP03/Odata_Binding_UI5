<mvc:View xmlns:table="sap.ui.table" controllerName="demo.ladera.odatabinding.controller.ListPage"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
    <List items = "{/Employees}">
    <items>
    <ObjectListItem title="{FirstName} {LastName}" intro = "{Title}" number="{EmployeeID}">
    </ObjectListItem>
    </items>
    </List>
    </Page>
</mvc:View>